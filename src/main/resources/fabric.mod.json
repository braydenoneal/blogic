{
	"schemaVersion": 1,
	"id": "blogic",
	"version": "${version}",
	"name": "Blogic",
	"description": "Minecraft automation and storage mod",
	"authors": [
		"Brayden O'Neal"
	],
	"license": "GPL-3.0",
	"icon": "assets/blogic/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"adapter": "kotlin",
				"value": "com.braydenoneal.Blogic"
			}
		],
		"client": [
			{
				"adapter": "kotlin",
				"value": "com.braydenoneal.BlogicClient"
			}
		],
		"fabric-datagen": [
			{
				"adapter": "kotlin",
				"value": "com.braydenoneal.BlogicDataGenerator"
			}
		]
	},
	"mixins": [],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.21.8",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	}
}
